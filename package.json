{
  "name": "vite-express-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch --import ./src/server/instrument.ts src/server.ts",
    "dev:server-only": "tsx watch src/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc src/server.ts --outDir dist --module ESNext --target ES2022 --moduleResolution bundler --allowImportingTsExtensions false && cp -r src/config dist/",
    "start": "NODE_ENV=production node --import ./dist/server/instrument.js dist/server.js",
    "preview": "npm run build && npm start",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@maxmind/geoip2-node": "^6.1.0",
    "@sentry/node": "^10.15.0",
    "@sentry/react": "^10.15.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "fastmcp": "^3.14.4",
    "http-proxy-middleware": "^3.0.5",
    "lightningcss": "^1.30.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.3"
  },
  "volta": {
    "node": "22.18.0"
  }
}
